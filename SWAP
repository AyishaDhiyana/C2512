SWAPPING 2 NUMBERS

pseudofunction :

function swapNumbers(int &number1,int &number2)
int temp = number1
number1 = number2
number2 = temp

function swapNumbersTest()
input number1,number2
swapNumbers(number1,number2)
print "number1"
print "number2"

pseudocode :
swapNumbersTest()

code :

#include <iostream>
#include <cmath>

using namespace std;

void swapNumbers(int &number1, int &number2)
{
    int temp = number1;
    number1 = number2;
    number2 = temp;
}

void swapNumbersTest()
{
    int number1,number2;
    cout<<"Enter number1 : ";
    cin>>number1;
    cout<<"Enter number2 : ";
    cin>>number2;
    
    swapNumbers(number1,number2);
    cout<<"new number :"<<number1<<endl;
    cout<<"new number :"<<number2<<endl;
}

int main()
{
    swapNumbersTest();
    return 0;
}
